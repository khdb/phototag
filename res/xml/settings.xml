<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" >

    <PreferenceCategory android:title="User profile" >
        <EditTextPreference
            android:key="prefUsername"
            android:summary="pref_user_name_summary"
            android:title="pref_user_name" />
    </PreferenceCategory>
    <PreferenceCategory android:title="pref_update_setting" >
        <CheckBoxPreference
            android:defaultValue="false"
            android:key="prefSendReport"
            android:summary="pref_send_report_summary"
            android:title="pref_send_report" >
        </CheckBoxPreference>

        <ListPreference
            android:entries="@array/syncFrequency"
            android:entryValues="@array/syncFrequencyValues"
            android:key="prefSyncFrequency"
            android:summary="ref_sync_frequency_summary"
            android:title="pref_sync_frequency" />
    </PreferenceCategory>

</PreferenceScreen>